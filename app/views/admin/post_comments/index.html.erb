<h4>コメント一覧</h4>
<% @post_comments.each do |comment| %>
<table>

 <%= comment.user.name %><%= comment.comment %><%= comment.created_at.strftime('%Y/%m/%d %H:%M') %>
 <%= link_to "削除", admin_post_comments_path(id: comment.id), method: :delete , data: { confirm: '本当に削除しますか?' } %></br>
</table>
<% end %>
