<div class="search_form">
  <%= form_with url: search_path, local: true, method: :get do |f| %>
   <%= f.text_field :word , placeholder: "キーワードを入力してください" %><!--検索内容をwordとしてアクションに送る-->
   <%= f.select :model, options_for_select([['User'],['Post']]) %> <!--複数のモデルを検索するための記述-->
   <%= f.submit "検索" %>
  <% end %>
</div>